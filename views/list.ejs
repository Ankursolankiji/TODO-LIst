<%- include("header") %> 
    <div class="box" id="heading">


        <h1>
            <%= listTitle %>
        </h1>
    </div>
    <div class="box">
       
        <ul>
            <% for (var i=0 ; i < newItems.length; i++ ){ %>
                <div class="item">
                    <input type="checkbox" name="" id="">
                    <p>
                        <%= newItems[i] %>
                    </p>
                </div>

                <% } ;%>
        </ul>
         <form action="/" method="post" class="item">
            
            <input type="text" name="newTask" id="task" placeholder="Add to the list" autocomplete="off">
            <button type="submit" name="list" value="<%= listTitle %> ">+</button>
        </form>
    </div>
    <%- include("footer") %> 
